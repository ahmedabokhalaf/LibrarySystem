<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="~/Content/twitter-bootstrap/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/Content/lib/css/all.min.css" />
    <link rel="stylesheet" href="~/Content/lib/css/StyleShared.min.css" />
    <title>@ViewBag.Title</title>
    @RenderSection("Style", false)
</head>
<body>
    <div class="row">
        <section class="col-1">
            @RenderSection("LeftPanel", false)
        </section>
        <main class="col-8">
            <partial name="_Header" />
            <main>
                @RenderBody()
            </main>
            <partial name="_Footer" />
        </main>
        <section class="col-3">
            @RenderSection("RightPanel", false)
        </section>
    </div>
    <script src="~/Content/lib/jquery/jquery/jquery.min.js"></script>
    <script src="~/Content/lib/jquery/jquery-validate/jquery.validate.min.js"></script>
    <script src="~/Content/lib/jquery/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/Content/jquery-ajax-unobtrusive/jquery.unobtrusive-ajax.min.js"></script>
</body>
</html>
